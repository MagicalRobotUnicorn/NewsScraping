<div class="container">
    <div class="row d-flex justify-content-center" id="titleRow">
            <h1 id="siteTitle">{{handleObject.title}}.</h1>
    </div>


    {{#each handleObject.responseArray}}
        <div class="row individualArticle" id="{{this.id}}">
            <div class="col-4 photoCol">
                <img src="{{this.thumb}}" class="articleImage" />
            </div>
            <div class="col-8 contentCol">
                <h5 class="headline">{{this.headline}}</h5>
                <p class="author">{{this.author}}</p>
                <p class="summary">{{this.summary}}</p>
                <p class="url">{{this.url}}</p>
                <button type="button" class="btn btn-outline-dark articleSelection" data-route="{{this.id}}">Explore Article</button>
            </div>
        </div>
    {{/each}}

</div>